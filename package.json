{
  "name": "stealth-input",
  "version": "1.0.0",
  "type": "module",
  "description": "Human-like input simulation utilities for realistic browser interactions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "playwright",
    "automation",
    "stealth",
    "human-like",
    "input",
    "simulation",
    "testing",
    "e2e"
  ],
  "author": "Sojan Varghese",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sojanvarghese/stealth-input.git"
  },
  "packageManager": "yarn@4.12.0",
  "peerDependencies": {
    "@playwright/test": "^1.57.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=4.12.0"
  }
}
